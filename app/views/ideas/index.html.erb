<h1>Ideas</h1>

<%= render partial: "layouts/idea_index", locals: { ideas: @ideas } %>

<%= form_with model: Idea.new do |form| %>
  <%= form.label :title %>
  <%= form.text_field :title %>
  <br><br>
  <%= form.label :description %>
  <%= form.text_area :description %>
  <%= form.submit %>
<% end %>